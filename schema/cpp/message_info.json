{
"name": "MessageInfo",
"type": "struct",
"comment": "Trace information relating to the current message",
"values": [
	{ "name": "source", "type": "uint8_t", "comment": "Source identifier (index into the list of connections)" },
	{ "name": "source_name", "type": "std::string_view", "comment": "Source name" },
	{ "name": "source_session_id", "type": "std::string_view", "comment": "Session identifier (UUID)" },
	{ "name": "source_seqno", "type": "uint64_t", "comment": "Sequence number (strictly increasing)" },
	{ "name": "receive_time_utc", "type": "std::chrono::nanoseconds", "comment": "Client receive time (realtime clock)" },
	{ "name": "receive_time", "type": "std::chrono::nanoseconds", "comment": "Client receive time (monotonic clock)" },
	{ "name": "source_send_time", "type": "std::chrono::nanoseconds", "comment": "Source send time (monotonic clock)" },
	{ "name": "source_receive_time", "type": "std::chrono::nanoseconds", "comment": "Source receive time (monotonic clock)" },
	{ "name": "origin_create_time", "type": "std::chrono::nanoseconds", "comment": "Origin create time (monotonic clock)" },
	{ "name": "origin_create_time_utc", "type": "std::chrono::nanoseconds", "comment": "Origin create time (realtime clock)" },
	{ "name": "is_last", "type": "bool", "comment": "Is last in batch?" },
	{ "name": "opaque", "type": "uint64_t", "comment": "Opaque value (internal)" }
]
}
