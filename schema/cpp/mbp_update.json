{
    "name": "MBPUpdate",
    "type": "struct",
    "comment": "Represents the update status of a single aggregate price level in the order book",
    "values": [
        {
            "name": "price",
            "type": "double",
            "comment": "Price level"
        },
        {
            "name": "quantity",
            "type": "double",
            "default": " = {}",
            "comment": "Total quantity available at price"
        },
        {
            "name": "implied_quantity",
            "type": "double",
            "default": " = NaN",
            "comment": "Total implied quantity at price (optional)"
        },
        {
            "name": "price_level",
            "type": "uint32_t",
            "default": " = {}",
            "comment": "Level of price (optional, 1-based indexing)"
        },
        {
            "name": "number_of_orders",
            "type": "uint32_t",
            "default": " = {}",
            "comment": "Number of orders at price (optional)"
        }
    ]
}
